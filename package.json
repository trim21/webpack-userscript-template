{
  "name": "webpack-userscript-template",
  "description": "Build your UserScript with webpack",
  "version": "0.0.0",
  "author": {
    "name": "Trim21",
    "email": "trim21me@gmail.com"
  },
  "scripts": {
    "postversion": "git push --follow-tags",
    "format": "prettier -w ./",
    "analize": "npm_config_report=true npm run build",
    "build": "webpack --mode production --config config/webpack.config.prod.cjs",
    "dev": "webpack --mode development --config config/webpack.config.dev.cjs"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/trim21/webpack-userscript-template"
  },
  "private": true,
  "dependencies": {
    "@trim21/gm-fetch": "^0.0.5",
    "jquery": "^3.6.1"
  },
  "devDependencies": {
    "@types/greasemonkey": "^4.0.4",
    "@types/jquery": "^3.5.14",
    "browserslist": "^4.21.4",
    "css-loader": "^6.7.1",
    "esbuild": "^0.15.10",
    "esbuild-loader": "^2.20.0",
    "less": "^4.1.3",
    "less-loader": "^11.0.0",
    "prettier": "^2.7.1",
    "style-loader": "^3.3.1",
    "typescript": "^4.8.4",
    "userscript-metadata-webpack-plugin": "^0.1.6",
    "webpack": "^5.74.0",
    "webpack-bundle-analyzer": "^4.6.1",
    "webpack-cli": "^4.10.0",
    "webpack-livereload-plugin": "^3.0.2",
    "webpack-merge": "^5.8.0",
    "webpack-sources": "^3.2.3"
  }
}
