    "@types/jquery": "^3.5.14",
    "babel-loader": "^8.2.5",
    "browserslist": "^4.20.3",
    "css-loader": "^6.7.1",
    "less": "^4.1.2",
    "less-loader": "^10.2.0",
    "style-loader": "^3.3.1",
    "typescript": "^4.6.4",
