{
  "name": "webpack-userscript-template",
  "description": "Build your UserScript with webpack",
  "version": "0.0.1",
  "author": {
    "name": "Trim21",
    "email": "trim21me@gmail.com"
  },
  "eslintIgnore": [
    "dist/*.js"
  ],
  "scripts": {
    "lint": "eslint src",
    "preversion": "npm run lint",
    "postversion": "git push --follow-tags",
    "anylize": "npm_config_report=true npm run build",
    "build": "webpack --mode production --config config/webpack.config.production.js",
    "dev": "webpack --mode development --config config/webpack.config.dev.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Trim21/webpack-userscript-template"
  },
  "private": true,
  "dependencies": {
    "axios": "0.21.1",
    "axios-userscript-adapter": "0.0.7",
    "jquery": "3.6.0"
  },
  "devDependencies": {
    "@babel/core": "^7.13.10",
    "@babel/preset-env": "^7.13.10",
    "@typescript-eslint/eslint-plugin": "4.18.0",
    "@typescript-eslint/parser": "4.18.0",
    "babel-loader": "^8.2.2",
    "browserslist": "^4.16.3",
    "css-loader": "5.1.3",
    "eslint": "7.22.0",
    "eslint-config-standard": "16.0.2",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "4.3.1",
    "eslint-plugin-standard": "4.1.0",
    "less": "4.1.1",
    "less-loader": "8.0.0",
    "style-loader": "2.0.0",
    "ts-loader": "8.0.18",
    "typescript": "4.2.3",
    "userscript-metadata-webpack-plugin": "0.1.0",
    "webpack": "^5.27.1",
    "webpack-bundle-analyzer": "4.4.0",
    "webpack-cli": "4.5.0",
    "webpack-livereload-plugin": "3.0.1",
    "webpack-merge": "5.7.3"
  }
}
